pipeline {
    agent any
    tools {
        maven 'Maven_4_0_0'
    }
    stages {
        stage('Build Stage') {
            steps {
                sh 'echo "Hello World"'
                sh 'echo "***Build Stage***"'
                sh 'mvn clean compile'
            }
        }

        stage('Test Stage') {
            steps {
                sh 'echo "Test Stage"'
             }
        }

        stage('Deploy Stage') {
            steps {
                sh 'echo "Deploy Stage"'
             }
        }

    }
}