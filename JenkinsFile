pipeline {
    agent any
    stages {
        stage('Build Stage') {
            steps {
                echo "Hello World"
                echo "***Build Stage***"

                withMaven(maven : 'maven_4_0_0') {
                    mvn clean compile
            }
        }
        }

        stage('Test Stage') {
            steps {
                echo "Test Stage"
             }
        }

        stage('Deploy Stage') {
            steps {
                echo "Deploy Stage"
             }
        }

    }
}